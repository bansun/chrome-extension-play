{
	"name": "BubbleUp",
	"manifest_version": 2,
	"description": "When you needed some distractions",
	"version": "0.0",
	"background": {
		"page": "background.html"
	},
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Bubble up your page"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["script.js"]
		}
	],
	"permissions": [
		"tabs"
	]
}